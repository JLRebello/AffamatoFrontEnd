<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">  


<servlet>
	<servlet-name>about</servlet-name>
	<servlet-class>aFE.aboutPageServlet</servlet-class>	
</servlet>    
<servlet-mapping>
	<servlet-name>about</servlet-name>
	<url-pattern>/about</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>dashboard</servlet-name>
	<servlet-class>aFE.dashboardPageServlet</servlet-class>	
</servlet>    
<servlet-mapping>
	<servlet-name>dashboard</servlet-name>
	<url-pattern>/dashboard</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>grocerylist</servlet-name>
	<servlet-class>aFE.grocerylistPageServlet</servlet-class>	
</servlet>    
<servlet-mapping>
	<servlet-name>grocerylist</servlet-name>
	<url-pattern>/grocerylist</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>inventory</servlet-name>
	<servlet-class>aFE.inventoryPageServlet</servlet-class>	
</servlet>    
<servlet-mapping>
	<servlet-name>inventory</servlet-name>
	<url-pattern>/inventory</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>recipes</servlet-name>
	<servlet-class>aFE.recipesPageServlet</servlet-class>	
</servlet>    
<servlet-mapping>
	<servlet-name>recipes</servlet-name>
	<url-pattern>/recipes</url-pattern>
</servlet-mapping>
  <welcome-file-list>
    <welcome-file>landingPage.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>